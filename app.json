    "slug": "gpa-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/small-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/small-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
